{
  "name": "uwniblog",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bunx @11ty/eleventy",
    "build-nocolor": "cross-env NODE_DISABLE_COLORS=1 bunx @11ty/eleventy",
    "build-ghpages": "bunx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
    "start": "bun run start:concurrent",
    "start:concurrent": "bunx @11ty/eleventy --serve --incremental",
    "start-ghpages": "bunx @11ty/eleventy --pathprefix=/eleventy-base-blog/ --serve --quiet",
    "debug": "cross-env DEBUG=Eleventy* bunx @11ty/eleventy",
    "debugstart": "cross-env DEBUG=Eleventy* bunx @11ty/eleventy --serve --quiet",
    "benchmark": "cross-env DEBUG=Eleventy:Benchmark* bunx @11ty/eleventy"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/luxon": "^3.7.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@myriaddreamin/typst-ts-node-compiler": "^0.6.1-rc2",
    "@myriaddreamin/typst.ts": "^0.6.1-rc2"
  }
}
